package hoja7;

/**
 *
 * @author Juan Pablo Merck
 * esta clase crea un objeto de tipo nodo
 * el cual tendra almacenara dos valores K y V
 */
public class Arbol <K,V>{
    private Nodo<K,V> root;
    
    //constructor de mi clase
    public Arbol(){
        
    }
    //setter y getter de la clase Arbol
    public void setRoot(Nodo<K,V> root){
        this.root=root;
    }
    public Nodo<K,V> getRoot(){
        return root;
    }
}
